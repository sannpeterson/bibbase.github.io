	author = {Hegedűs, Csaba and Kovács, Katalin and Polgár, Zsuzsanna and Regdon, Zsolt and Szabó, Éva and Robaszkiewicz, Agnieszka and Forman, Henry Jay and Martner, Anna and Virág, László},
