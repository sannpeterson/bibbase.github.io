	urldate = {2018-07-11TZ},
	journal = {European Surgery},
	author = {Putzer, D. and Schullian, P. and Braunwarth, E. and Fodor, M. and Primavesi, F. and Cardini, B. and Resch, T. and Oberhuber, R. and Maglione, M. and Margreiter, C. and Schneeberger, S. and Stättner, S. and Öfner-Velano, D. and Jaschke, W. and Bale, R. J.},
