	note = {00004 },
	keywords = {Aged, Combined Modality Therapy, Disease Progression, Follow-Up Studies, High-Intensity Focused Ultrasound Ablation, Humans, Male, Middle Aged, Multimodal Imaging, Neoplasm Grading, Prognosis, Prostate-Specific Antigen, Prostatic Neoplasms, Radiotherapy, Adjuvant, Survival Analysis, Treatment Outcome},
